{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": false,
      "target": "es2017",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "paths": {
        "@app/*": ["apps/*/src"],
        "@libs/*": ["libs/*/src"]
      }
    }
  }
  